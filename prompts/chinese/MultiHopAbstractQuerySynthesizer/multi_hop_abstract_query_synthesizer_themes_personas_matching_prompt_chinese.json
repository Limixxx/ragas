{
  "ragas_version": "0.4.1",
  "original_hash": 484924909288144619,
  "language": "chinese",
  "instruction": "给定一个主题列表和带有其角色的人物角色，根据其角色描述将每个人物角色与相关主题关联起来。",
  "examples": [
    {
      "input": {
        "themes": [
          "同理心",
          "包容性",
          "远程工作"
        ],
        "personas": [
          {
            "name": "人力资源经理",
            "role_description": "专注于包容性和员工支持。"
          },
          {
            "name": "远程团队负责人",
            "role_description": "管理远程团队沟通。"
          }
        ]
      },
      "output": {
        "mapping": {
          "HR Manager": [
            "包容性",
            "同理心"
          ],
          "Remote Team Lead": [
            "远程工作",
            "同理心"
          ]
        }
      }
    }
  ]
}